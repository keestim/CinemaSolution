        </from-view-id>

        <navigation-case>
            <from-outcome>pay now</from-outcome>
            <to-view-id>/user/bookingPayment.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>pay later</from-outcome>
            <to-view-id>/user/bookingSuccessful.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>failure</from-outcome>
